#include<bits/stdc++.h>
using namespace std;
#define int long long
const int N=2e5+10;
vector<vector<int>>arr(N+1);
vector<int>cnt(N+1,0);
signed main(){
	ios::sync_with_stdio(false);
	cin.tie(0);
	for(int i=2;i<=N;i++){
		if(!arr[i].empty()){
			continue;
		}
		for(int j=i;j<=N;j+=i){
			arr[j].push_back(i);
		}
	}
	int t;
	cin>>t;
	while(t--){
		int ans=2;
		int n;
		cin>>n;
		vector<int>a(n),b(n);
		for(int i=0;i<n;i++){
			cin>>a[i];
		}
		for(int i=0;i<n;i++){
			cin>>b[i];
		}
		for(int i=0;i<n;i++){
			for(int x:arr[a[i]]){
				if(cnt[x]>0){
					ans=0;
				}
				cnt[x]++;
			}
		}
		for(int i=0;i<n;i++){
			for(int x:arr[a[i]]){
				cnt[x]--;
			}
			for(int x:arr[a[i]+1]){
				if(cnt[x]>0){
					ans=min(ans,1ll);
				}
			}
			for(int x:arr[a[i]]){
				cnt[x]++;
			}
		}
		for(int i=0;i<=N;i++){
			cnt[i]=0;
		}
		cout<<ans<<'\n';
	}
}
